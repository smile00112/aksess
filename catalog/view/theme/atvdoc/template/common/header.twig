					} else {
						dropdownMenu.css({"right": "0"});
					}
				}
				$(this).children(".dropdown-menu").stop(true, true).delay(200).fadeIn(150);
			},
			mouseleave: function() {
				$(this).children(".dropdown-menu").stop(true, true).delay(200).fadeOut(150);
